{
  "$schema": "https://raw.githubusercontent.com/Exit-9B/InventoryInjector/main/docs/InventoryInjector.schema.json",
  "rules": [
    {
      "match": {
        "formType": "Book",
        "formId": "Dragonborn.esm|1CAF2"
      },
      "assign": {
        "iconLabel": "book_map"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
        "formId": {
          "anyOf": [
            "Dragonborn.esm|1CAC0",
            "Dragonborn.esm|1CAC1"
          ]
        }
      },
      "assign": {
        "subType": "dragonclaw"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
        "formId": "Dragonborn.esm|2BAAE"
      },
      "assign": {
        "iconLabel": "book_map"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
        "formId": "Dragonborn.esm|247F9"
      },
      "assign": {
        "iconLabel": "misc_trollskull"
      }
    },
    {
      "match": {
        "formType": "Weapon",
        "formId": {
          "anyOf": [
            "Dragonborn.esm|179C9",
            "Dragonborn.esm|206F2",
            "Dragonborn.esm|398E6"
          ]
        }
      },
      "assign": {
        "subType": "pickaxe"
      }
    }
  ]
}
